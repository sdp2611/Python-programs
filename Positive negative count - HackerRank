def plusMinus(arr):
    a=0
    b=0
    c=0
    n=len(arr)
    for i in arr:
        if(i>0):
            a=a+1
        elif(i<0):
            b=b+1
        else:
            c=c+1
    print(a/n)
    print(b/n)
    print(c/n)
        

if __name__ == '__main__':
    n = int(input().strip())

    arr = list(map(int, input().rstrip().split()))

    plusMinus(arr)

