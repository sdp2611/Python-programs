import math
import os
import random
import re
import sys



def miniMaxSum(arr):
    sum=0
    arr.sort()
    for i in range(5):
        sum=sum+arr[i]
        if(i==3):
            print(sum,end=' ')
    print(sum-arr[0])
        

if __name__ == '__main__':

    arr = list(map(int, input().rstrip().split()))

    miniMaxSum(arr)
